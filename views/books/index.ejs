<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Your Books</title>
</head>
<body>
  <%- include('../partials/_navbar.ejs') %>
  <h1>Your Books</h1>
  <a href="/users/<%=user._id%>/books/new">Add a Book</a>
  <ul>
    <% books.forEach((book)=>{ %>
      <li><%= book.title %> by <%= book.Author %></li>
    <% }) %>
  </ul>
</body>
</html>